{
  "name": "qv",
  "description": "qv",
  "repository": {
    "type": "git",
    "url": "git@github.com:cblanc/qv"
  },
  "files": [
    "dist/**/*.js"
  ],
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test node_modules/.bin/nyc node_modules/.bin/mocha",
    "lint": "node_modules/.bin/tslint -c tslint.json -p tsconfig.json lib/**/*.ts",
    "build": "npm run lint && node_modules/.bin/tsc",
    "coverage": "node_modules/.bin/nyc report --reporter=text-lcov | node_modules/.bin/coveralls || true",
    "prepare": "npm run build",
    "watch": "node_modules/.bin/mocha --watch-extensions ts --watch",
    "update!": "node_modules/.bin/ncu -u && npm install && npm run test"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "mocha": {
    "spec": "tests/*.ts",
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=10.0"
  },
  "author": "Christopher Blanchard",
  "license": "MIT",
  "devDependencies": {
    "@cablanchard/tsconfig": "~1.0.0",
    "@types/chai": "~4.2.0",
    "@types/chai-as-promised": "~7.1.0",
    "@types/mocha": "~5.2.5",
    "@types/node": "~12.7.0",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "coveralls": "~3.0.2",
    "mocha": "~6.2.0",
    "npm-check-updates": "~3.1.0",
    "nyc": "~14.1.0",
    "prettier": "~1.18.0",
    "source-map-support": "~0.5.9",
    "ts-node": "~8.3.0",
    "tslint": "~5.20.0",
    "typescript": "~3.6.2"
  },
  "dependencies": {}
}
